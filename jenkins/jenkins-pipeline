pipeline { 
  agent any
  
  stages {
    stage( 'clone reposirtory'){
        steps{
            git url: 'https://github.com/Kkigathi/IP'
        }
    }
    stage ('Dependencies'){
        steps{
            sh 'npm install'
        }
    }
    stage('Build'){
        steps{
            sh 'node server'
        }
    }
  }
}